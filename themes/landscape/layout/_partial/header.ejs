<header class="header">
	<a href="<%- url_for() %>" class="header__title">
		<%= config.title %></a>
	</a>
	<h2 class="header__subtitle">
		<%= theme.subtitle %>
	</h2>
	<nav>
		<a id="main-nav-toggle" class="nav-icon"></a>
		<% for (var i in theme.menu) { %>
			<a class="main-nav-link" href="<%- url_for(theme.menu[i]) %>"><%= i %></a>
		<% } %>
	</nav>
</header>

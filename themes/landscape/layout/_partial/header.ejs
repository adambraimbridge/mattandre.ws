<header class="header">
	<a href="<%- url_for() %>" class="header__title"><%= config.title %></a></a>
	<div class="header__subtitle"><%= theme.subtitle %></div>
	<nav class="header__nav">
		<% for (var i in theme.menu) { %>
			<a class="header__nav-item header__nav-item--importance-<%= theme.menu[i].importance %>" href="<%- url_for(theme.menu[i].url) %>"><%= i %></a>
		<% } %>
	</nav>
</header>
